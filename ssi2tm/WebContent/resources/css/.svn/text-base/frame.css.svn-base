body{
	font-family:  verdana,tahoma，宋体;
	font-size: 12px;
	color: #000000;
	margin-left: 0px;
	margin-top: 0px;
	margin-right: 0px;
	margin-bottom: 0px;
	background-position:center;/*背景图像定位，可选参数top，center，bottom，left，center，right*/
	background-color: #ffffff;
