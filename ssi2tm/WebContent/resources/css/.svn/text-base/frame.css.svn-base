/*其他*/
.left {
	background-attachment: scroll;
	background-color: #A9120E;
	background-repeat: no-repeat;
	background-position: left top;
}
.top1{
	font-size: 12px;
	color: #000000;
	background-image: url(./../images/top_m.gif);
	text-align: center;
	height: 30px;
	cursor:hand;
	vertical-align: middle;
}
.top2 {
	font-size: 12px;
	color: #000000;
	background-image: url(./../images/top_m2.gif);
	text-align: center;
	height: 30px;
