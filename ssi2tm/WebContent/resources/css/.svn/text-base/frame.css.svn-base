TABLE {
	FONT-FAMILY:verdana,tahoma，宋体; FONT-SIZE: 12px;COLOR: #000000; LINE-HEIGHT: 150%
}
TD {
	FONT-FAMILY: verdana,tahoma，宋体; FONT-SIZE: 12px; LINE-HEIGHT: 150%
}
TR {
	FONT-FAMILY:verdana,tahoma，宋体; FONT-SIZE: 12px; LINE-HEIGHT: 150%
}
a:link{
    color:#000000;
	text-decoration:none;
}
a:visited{
	text-decoration:none;
	 color:#000000;
}
a:active{
	text-decoration:none;
	 color:#000000;
}
a:hover{
	text-decoration: underline;
	color:#000000;
}
.left_m {
	COLOR: #000000;
	TEXT-DECORATION: none;
	font-size: 12px;
	height: 26px;
	width: 176px;
	background-image: url(./../images/left11.gif);
	background-repeat: no-repeat;
	text-indent: 30px;
	cursor:hand;
}
.left_m  A:link {
	COLOR: #000000;
	TEXT-DECORATION: none;
}
.left_m  A:visited {
	COLOR: #000000;
	TEXT-DECORATION: none;
}
.left_m A:hover {
    COLOR: #000000;
	text-decoration: underline;
}
